{
    "manifest_version": 3,
    "name": "Simple Proxy",
    "version": "1.0",
    "permissions": [
      "proxy",
      "tabs",
      "storage",
      "webRequest",
      "declarativeNetRequest",
      "declarativeNetRequestFeedback"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icon.png"
      }
    },
    "icons": {
      "48": "icon.png"
    },
    "declarative_net_request": {
      "rule_resources": [{
        "id": "ruleset1",
        "enabled": true,
        "path": "rules.json"
      }]
    }
  }
  